#include <iostream>
using namespace std;
struct book 
{
    int id;
    int page;
    float price;
    
};
int main() {
    book b1,b2;
   cout<<"the details of book no 1 is :";
    cin>> b1.id>> b1.page>> b1.price;
      cout<<"the details of book no 2 is :";
    cin>> b2.id>> b2.page>> b2.price;
    cout<<"this is book 1"<< b1.id<<" " << b1.page<<" "<< b1.price<<endl;
    cout<<"this is book 2"<<b2.id <<" "<< b2.page<<" "<< b2.price;

    return 0;
}
